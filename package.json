{
  "name": "whatsapp-desktop",
  "description": "Bringing WhatsApp to desktop, inspired by WhatsDesk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\" --config .prettierrc.json",
    "lint": "eslint \"src/**/*.{ts,tsx,css}\" --fix",
    "prettify": "yarn run format && yarn run lint",
    "start": "electron main.js"
  },
  "dependencies": {
    "electron": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "yarn-upgrade-all": "^0.6.1"
  }
}